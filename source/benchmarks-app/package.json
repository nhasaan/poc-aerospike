  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.15",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.5.0",
